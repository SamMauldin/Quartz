write("Username:")
local user = read()

write("Are you sure?[y/n]:")
local del = read()
if del == "y" else
	fs.delete("/Home/" .. user)
	local admins = textutils.unserialize(file.read("/System/admins", nil))
	for k,v in pairs(admins) do
		if v == user then
			admins[k] = nil
		end
	end
	file.write("/System/admins", textutils.serialize(admins))
else
	print("Aborted")
end